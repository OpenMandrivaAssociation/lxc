Subject: [PATCH] Add an OpenMandriva LX template

---
 configure.ac                  |    1 +
 templates/Makefile.am         |    1 +
 templates/Makefile.in         |    7 +-
 templates/lxc-openmandriva.in | 1271 +++++++++++++++++++++++++++++++++++++++++
 4 files changed, 1278 insertions(+), 2 deletions(-)
 create mode 100644 templates/lxc-openmandriva.in

diff --git a/configure.ac b/configure.ac
index 7dc82cf..f8c14ff 100644
--- a/configure.ac
+++ b/configure.ac
@@ -361,6 +361,7 @@ AC_CONFIG_FILES([
 	templates/lxc-opensuse
 	templates/lxc-busybox
 	templates/lxc-fedora
+	templates/lxc-openmandriva
 	templates/lxc-oracle
 	templates/lxc-altlinux
 	templates/lxc-sshd
diff --git a/templates/Makefile.am b/templates/Makefile.am
index 0c30667..3f03706 100644
--- a/templates/Makefile.am
+++ b/templates/Makefile.am
@@ -6,6 +6,7 @@ templates_SCRIPTS = \
 	lxc-ubuntu-cloud \
 	lxc-opensuse \
 	lxc-fedora \
+	lxc-openmandriva \
 	lxc-oracle \
 	lxc-altlinux \
 	lxc-busybox \
diff --git a/templates/Makefile.in b/templates/Makefile.in
index c61c3b4..a3301a5 100644
--- a/templates/Makefile.in
+++ b/templates/Makefile.in
@@ -41,7 +41,7 @@ DIST_COMMON = $(srcdir)/Makefile.am $(srcdir)/Makefile.in \
 	$(srcdir)/lxc-debian.in $(srcdir)/lxc-fedora.in \
 	$(srcdir)/lxc-opensuse.in $(srcdir)/lxc-oracle.in \
 	$(srcdir)/lxc-sshd.in $(srcdir)/lxc-ubuntu-cloud.in \
-	$(srcdir)/lxc-ubuntu.in
+	$(srcdir)/lxc-ubuntu.in $(srcdir)/lxc-openmandriva.in
 ACLOCAL_M4 = $(top_srcdir)/aclocal.m4
 am__aclocal_m4_deps = $(top_srcdir)/config/acinclude.m4 \
 	$(top_srcdir)/configure.ac
@@ -50,7 +50,7 @@ am__configure_deps = $(am__aclocal_m4_deps) $(CONFIGURE_DEPENDENCIES) \
 mkinstalldirs = $(install_sh) -d
 CONFIG_HEADER = $(top_builddir)/src/config.h
 CONFIG_CLEAN_FILES = lxc-debian lxc-ubuntu lxc-ubuntu-cloud \
-	lxc-opensuse lxc-busybox lxc-fedora lxc-oracle lxc-altlinux \
+	lxc-opensuse lxc-busybox lxc-fedora lxc-openmandriva lxc-oracle lxc-altlinux \
 	lxc-sshd lxc-archlinux lxc-alpine
 CONFIG_CLEAN_VPATH_FILES =
 am__vpath_adj_setup = srcdirstrip=`echo "$(srcdir)" | sed 's|.|.|g'`;
@@ -225,6 +225,7 @@ templates_SCRIPTS = \
 	lxc-ubuntu-cloud \
 	lxc-opensuse \
 	lxc-fedora \
+	lxc-openmandriva \
 	lxc-oracle \
 	lxc-altlinux \
 	lxc-busybox \
@@ -277,6 +278,8 @@ lxc-busybox: $(top_builddir)/config.status $(srcdir)/lxc-busybox.in
 	cd $(top_builddir) && $(SHELL) ./config.status $(subdir)/$@
 lxc-fedora: $(top_builddir)/config.status $(srcdir)/lxc-fedora.in
 	cd $(top_builddir) && $(SHELL) ./config.status $(subdir)/$@
+lxc-fedora: $(top_builddir)/config.status $(srcdir)/lxc-openmandriva.in
+	cd $(top_builddir) && $(SHELL) ./config.status $(subdir)/$@
 lxc-oracle: $(top_builddir)/config.status $(srcdir)/lxc-oracle.in
 	cd $(top_builddir) && $(SHELL) ./config.status $(subdir)/$@
 lxc-altlinux: $(top_builddir)/config.status $(srcdir)/lxc-altlinux.in
